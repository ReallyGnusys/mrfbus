killall zl_0
killall host_1
killall usbrf_2
killall rf_20
killall rf_2f


make clean
make DEVTYPE=zl MRFID=0
cp mtest zl_0

make clean
make DEVTYPE=host MRFID=1
cp mtest host_1

make clean
make DEVTYPE=usbrf MRFID=2
cp mtest usbrf_2

make clean
make DEVTYPE=rf MRFID=0x20
cp mtest rf_20

make clean
make DEVTYPE=rf MRFID=0x2f
cp mtest rf_2f


